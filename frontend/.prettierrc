{
  "endOfLine": "lf",
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "es5",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "semi": true,
  "tailwindConfig": "./tailwind.config.js",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@components/(.*)$",
    "^@configs/(.*)$",
    "^@layouts/(.*)$",
    "^@mocks/(.*)$",
    "^@models/(.*)$",
    "^@pages/(.*)$",
    "^@routes/(.*)$",
    "^@services/(.*)$",
    "^@stores/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
