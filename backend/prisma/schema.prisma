generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Fatura {
  id                          Int      @id @default(autoincrement())
  numero_cliente              String
  mes_referencia              DateTime
  energia_eletrica_kwh        Float
  energia_eletrica_valor      Float
  energia_sceee_kwh           Float
  energia_sceee_valor         Float
  energia_compensada_kwh      Float
  energia_compensada_valor    Float
  contribu_ilum_publica_valor Float
  fileUrl                     String?
}
